<?xml
  version="1.0"
  encoding="utf-8"?>

<mauiIssueRenderersDoNotWorkWithGenericClasses:CustomFlyoutPage
  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  xmlns:mauiIssueRenderersDoNotWorkWithGenericClasses="clr-namespace:maui_issue_renderers_do_not_work_with_generic_classes"
  x:Class="maui_issue_renderers_do_not_work_with_generic_classes.AppFlyoutPage"
  FlyoutLayoutBehavior="Popover">
  <FlyoutPage.Flyout>
    <mauiIssueRenderersDoNotWorkWithGenericClasses:FlyoutMenuPage x:Name="flyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <mauiIssueRenderersDoNotWorkWithGenericClasses:MainPage BackgroundColor="Green" />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</mauiIssueRenderersDoNotWorkWithGenericClasses:CustomFlyoutPage>
